<?xml version="1.0"?>
<extension key="io.compuco.eventify" type="module">
  <file>eventify</file>
  <name>Eventify Integration</name>
  <description>A CiviCRM extension that intergrate CiviCRM with Eventify</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Compuco</author>
    <email>hello@compucorp.co</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/compucorp/io.compuco.eventify</url>
    <url desc="Documentation">https://github.com/compucorp/io.compuco.eventify/blob/master/readme.md</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-05-03</releaseDate>
  <version>1.0.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.39.1</ver>
  </compatibility>
  <comments>This is a new, undeveloped module</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Eventify</namespace>
    <format>22.05.2</format>
    <angularModule>crmEventify</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>menu-xml@1.0.0</mixin>
  </mixins>
</extension>
